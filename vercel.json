{
  "version": 2,
  "routes": [
    {
      "src": "/thank-you",
      "dest": "/thank-you.html"
    },
    {
      "src": "/downloads/(.*)",
      "dest": "/downloads/$1"
    },
    {
      "src": "/app",
      "dest": "/app/index.html"
    },
    {
      "src": "/app/(.*)",
      "dest": "/app/$1"
    }
  ]
}
